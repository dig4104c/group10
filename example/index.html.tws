tle);\u000a						  trackTitle.appendChild(trackTitleText);\u000a						  \u000a\u000a							audioContainer.insertBefore(trackTitle,audioContainer.childNodes[0]);\u000a\u000a\u000a						});\u000a						\u000a						\u000a						\u000a						var timer = document.createElement("div");\u000a						timer.id = "timer";\u000a						timer.innerHTML = "00:00";\u000a						audioContainer.appendChild(timer);\u000a						\u000a						function msToDate(ms) {\u000a							var date = new Date(ms);\u000a\u000a							var m = date.getMinutes();\u000a							if (m < 10) {\u000a								m = "0" + m;\u000a							}\u000a							var s = date.getSeconds();\u000a							if (s < 10) {\u000a								s = "0" + s;\u000a							}\u000a							\u000a							return m + ":" + s; \u000a							\u000a						}\u000a						\u000a						function updateProgress(pos, dur) {\u000a							progress.setAttribute("value",pos/dur*100);\u000a							timer.innerHTML = msToDate(pos);\u000a							\u000a						}\u000a						\u000a						play = document.createElement("button");\u000a						play_text = document.createTextNode("");\u000a						play.setAttribute("class","fa fa-play");\u000a						play.id = "play";\u000a						play.appendChild(play_text);\u000a						audioContainer.appendChild(play);\u000a						play.onclick = function(){\u000a							sound.play({\u000a									whileplaying: function() {\u000a										updateProgress( this.position, this.duration );\u000a\u000a\u000a\u000a										\u000a											\u000a											if($('#audio').length) {\u000a												\u000a											} else {\u000a												sound.stop();\u000a\u000a												$(this).parent().remove();\u000a    \u000a\u000a\u000a\u000a											}\u000a\u000a\u000a	\u000a										\u000a\u000a									  }\u000a							});\u000a							\u000a							audioContainer.insertBefore(pause,play);\u000a							audioContainer.removeChild(play);\u000a			\u000a						};\u000a				\u000a						pause = document.createElement("button");\u000a						pause_text = document.createTextNode("");\u000a						pause.setAttribute("class","fa fa-pause");\u000a						pause.id = "pause";\u000a						pause.appendChild(pause_text);\u000a						pause.onclick = function(){\u000a							sound.pause();\u000a							\u000a							audioContainer.insertBefore(resume,pause);\u000a							audioContainer.removeChild(pause);\u000a							\u000a						};\u000a				\u000a						resume = document.createElement("button");\u000a						resume_text = document.createTextNode("");\u000a						resume.setAttribute("class","fa fa-play");\u000a						resume.id = "resume";\u000a						resume.appendChild(resume_text);\u000a						\u000a						resume.onclick = function(){\u000a							sound.resume();\u000a							audioContainer.insertBefore(pause,resume);\u000a							audioContainer.removeChild(resume);\u000a							\u000a						};\u000a						\u000a						var progress = document.createElement("progress");\u000a						progress.setAttribute("value", "0");\u000a						progress.setAttribute("max", "100");\u000a						\u000a								audioContainer.appendChild(progress);\u000a\u000a						\u000a						\u000a						\u000a						\u000a\u000a				\u000a				});\u000a		}, 3000);\u000a\u000a\u000a\u000a		place.parentNode.insertBefore(audioContainer,place);\u000a		$(audioContainer).hide();\u000a\u000a		\u000a\u000a	\u000a\u000a\u000a	}\u000a}\u000a
